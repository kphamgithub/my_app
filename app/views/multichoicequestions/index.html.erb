<h2>Admin only</h2>
	<% @multichoicequestions.each do |q| %>
	<p> Question id:
	<%= q.id %>
	</p>
    <p> Name: 
	<%= q.name %>
	</p>
	<%= button_to "Delete ", { controller: "multichoicequestions", action: "destroy", id: q.id }, method: :delete, data: { confirm: "Are you sure?" } %>
        <% end %>
	<p><%= link_to "new multichoice question", new_multichoicequestion_path %></p>
